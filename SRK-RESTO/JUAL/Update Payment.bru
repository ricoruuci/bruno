meta {
  name: Update Payment
  type: http
  seq: 4
}

patch {
  url: {{API_URL}}/payment
  body: json
  auth: inherit
}

body:json {
  {
    "nota_jual": "202506230001",
    "payment_type" : 0
    /*
    0 = QRIS
    1 = DEBIT need payment code (BCA/MANDIRI/etc) * number
    2 = CREDIT need payment code (BCA/MANDIRI/etc) * number
    3 = CASH
    */
  }
}
